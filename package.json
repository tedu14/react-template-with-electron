{
  "name": "electron-with-react",
  "version": "0.1.0",
  "private": true,
  "main": "src/main.js",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "styled-components": "^5.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "elec": "cross-env NODE_ENV=dev wait-on 'http://localhost:3000' && nodemon --exec electron .",
    "web": "cross-env BROWSER=none yarn start",
    "dev": "concurrently \"yarn web\" \"yarn elec\"",
    "postinstall": "electron-builder install-app-deps",
    "electron-build": "electron-builder -mwl"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "electron-builder": "^22.6.1",
    "electron-is-dev": "^1.2.0",
    "electron-reload": "^1.5.0",
    "nodemon": "^2.0.4",
    "wait-on": "^5.0.0"
  },
  "author": {
    "name": "Seu nome",
    "email": "seu.email@email.com",
    "url": "https://seu-site.com"
  },
  "homepage": "./",
  "build": {
    "appId": "com.seu-site.seu-app",
    "productName": "MyApp",
    "copyright": "Copyright Â© 2019 Seu Nome",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
